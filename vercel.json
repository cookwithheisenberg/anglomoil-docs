{
  "version": 2,
  "builds": [
    {
      "src": "wireframes/pages/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "wireframes/pages/document-detail.html",
      "use": "@vercel/static"
    },
    {
      "src": "wireframes/pages/filtered-view.html",
      "use": "@vercel/static"
    },
    {
      "src": "wireframes/components/navigation.html",
      "use": "@vercel/static"
    },
    {
      "src": "wireframes/css/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "wireframes/js/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/document/:documentName",
      "dest": "/wireframes/pages/document-detail.html"
    },
    {
      "src": "/filtered-view(.*)",
      "dest": "/wireframes/pages/filtered-view.html"
    },
    {
      "src": "/(.*)",
      "dest": "/wireframes/pages/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/document/:documentName",
      "destination": "/wireframes/pages/document-detail.html"
    },
    {
      "source": "/filtered-view",
      "destination": "/wireframes/pages/filtered-view.html"
    },
    {
      "source": "/filtered-view/:category",
      "destination": "/wireframes/pages/filtered-view.html?category=:category"
    },
    {
      "source": "/",
      "destination": "/wireframes/pages/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 