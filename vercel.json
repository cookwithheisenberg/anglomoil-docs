{
  "outputDirectory": "wireframes",
  "buildCommand": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/pages/main-hub.html"
    },
    {
      "source": "/document",
      "destination": "/pages/document-detail.html"
    },
    {
      "source": "/filtered-view",
      "destination": "/pages/filtered-view.html"
    },
    {
      "source": "/components/:path*",
      "destination": "/components/:path*"
    },
    {
      "source": "/css/:path*",
      "destination": "/css/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "/js/:path*"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
} 