{
  "version": 2,
  "builds": [
    {
      "src": "wireframes/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/document",
      "destination": "/wireframes/pages/document-detail.html"
    },
    {
      "source": "/filtered-view",
      "destination": "/wireframes/pages/filtered-view.html"
    },
    {
      "source": "/",
      "destination": "/wireframes/pages/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 